{
  "name": "cordova-sqlite-storage-ext-commoncore",
  "version": "1.0.0-dev",
  "description": "Native interface to SQLite for PhoneGap/Cordova - cordova-sqlite-storage-ext-commoncore plugin version branch",
  "cordova": {
    "id": "cordova-sqlite-storage-ext-commoncore",
    "platforms": [
      "android",
      "ios",
      "osx",
      "windows"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpbrew/cordova-sqlite-storage.git"
  },
  "keywords": [
    "sqlite",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios",
    "cordova-osx",
    "cordova-windows"
  ],
  "author": {
    "name": "various contributors",
    "email": "sales@xpbrew.consulting"
  },
  "bugs": {
    "url": "https://github.com/xpbrew/cordova-sqlite-storage/issues"
  },
  "homepage": "https://github.com/xpbrew/cordova-sqlite-storage",
  "license": "MIT",
  "scripts": {
    "clean-spec": "rm -rf spec/[mnp]* && git cl spec/config.xml && git st",
    "prepare-js": "coffee -p SQLitePlugin.coffee.md > www/SQLitePlugin.js",
    "prepare-spec": "node scripts/prepareSpec.js",
    "spec-help": "echo 'do `npm run prepare-spec` then build & run test app in `spec`'",
    "test": "echo 'npm test is not supported' && npm run spec-help && exit 1"
  },
  "dependencies": {
    "cordova-sqlite-storage-dependencies": "2.0.0"
  },
  "devDependencies": {
    "coffeescript": "1",
    "cross-spawn": "^6.0.5",
    "fs-extra": "^7.0.1"
  }
}
